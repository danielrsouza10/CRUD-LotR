<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    controllerName="ui5.o_senhor_dos_aneis.controller.List"
>
    <Dialog
        id="filterDialog"
        title="Filter"
    >
        <Select
            id="comboBoxRacas"
            change=".onChangeComboBoxRacas"
            items="{racas>/}"
        >
            <core:Item
                key="{racas>id}"
                text="{racas>nome}"
            />
        </Select>
        <Select
            id="profissaoComboBox"
            width="20%"
            change=".onChangeProfissaoBoxRacas"
        >
            <core:Item
                key="0"
                text="Nenhum"
            />
            <core:Item
                key="1"
                text="Guerreiro"
            />
            <core:Item
                key="2"
                text="Arqueiro"
            />
            <core:Item
                key="3"
                text="Mago"
            />
            <core:Item
                key="4"
                text="Ladrao"
            />
            <core:Item
                key="5"
                text="Jardineiro"
            />
            <core:Item
                key="6"
                text="Aventureiro"
            />
            <core:Item
                key="7"
                text="Rei"
            />
            <core:Item
                key="8"
                text="Senhora de Lothlorien"
            />
            <core:Item
                key="9"
                text="Senhor de Valfenda"
            />
            <core:Item
                key="10"
                text="Escudeira"
            />
            <core:Item
                key="11"
                text="Capitao"
            />
            <core:Item
                key="12"
                text="Princesa"
            />
            <core:Item
                key="13"
                text="Ent"
            />
            <core:Item
                key="14"
                text="Cavaleiro"
            />
            <core:Item
                key="15"
                text="Ex-Hobbit"
            />
        </Select>
        <u:Calendar
            id="calendar"
            select="handleCalendarSelect"
            intervalSelection="true"
            weekNumberSelect="handleWeekNumberSelect"
        />
        <beginButton>
            <Button
                text="Ok"
                press=".onCloseFilterDialog"
            />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>
